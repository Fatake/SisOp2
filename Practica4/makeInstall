imprimerArchivo: comandos
    touch file.txt

comandos: 
  @echo "ls -al"
  ls -al
  @echo "CD /Documents  and pwd"
  pwd
  cd ~/Documents
  pwd
  @echo "CP"
  cp hola.txt hola2.txt
  @echo "mv "
  mv hola2.txt hola.txt
  @echo "mkdir"
  mkdir ejemploDir
  @echo "rm"
  rm hola.txt
  @echo "rmdir ejemploDir"
  rmdir ejemploDir
  @echo "cat"
  cat hola.txt hola2.txt
  @echo "more"
  more hola.txt hola2.txt
  @echo "head"
  head hola.txt
  @echo "tail"
  tail hola.txt
  @echo "diff"
  diff hola.txt hola2.txt
  @echo "lpr y lpq"
  lpr hola.txt ... nuevo.txt
  lpq
  @echo "cmp"
  cmp bin1 bin2
  @echo "date"
  date
  @echo "who"
  who
  @echo "ps"
  ps -aux
  @echo "file"
  file hola.txt
  @echo "kill"
  kill 9167
  @echo "df"
  df
  @echo "man"
  man lpq